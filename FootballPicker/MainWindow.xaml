<Window x:Class="FootballPicker.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:FootballPicker"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
    <ListBox HorizontalAlignment="Left" Height="245" Margin="10,64,0,0" VerticalAlignment="Top" Width="497" ItemsSource="{Binding GameWinners}">
        <ListBox.ItemTemplate>
            <DataTemplate>
                <StackPanel>
                    <TextBlock Text="{Binding Path=Name}"/>
                </StackPanel>
            </DataTemplate>
        </ListBox.ItemTemplate>
        <TextBlock Height="100" TextWrapping="Wrap" Text="TextBlock" Width="100"/>
    </ListBox>
</Window>
